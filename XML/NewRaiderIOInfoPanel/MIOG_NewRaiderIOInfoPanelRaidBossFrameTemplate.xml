<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">    
    <Frame name="MIOG_NewRaiderIOInfoPanelRaidBossFrameTemplate" virtual="true">
        <Layers>
            <Layer level="BORDER">
                <Texture parentKey="Border" setAllPoints="true">
                    <Size x="18" y="18" />
                    <Color r="1" g="0" b="0" a="1" />
                </Texture>
            </Layer>

            <Layer level="ARTWORK">
                <Texture parentKey="Icon" enableMouseClicks="true" desaturated="true">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Border" relativePoint="TOPLEFT" x="1" y="-1" />
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Border" relativePoint="BOTTOMRIGHT" x="-1" y="1" />
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Index" font="GameFontHighlight" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="1" y="-1"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>