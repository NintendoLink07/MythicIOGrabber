<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GroupOrganizerRaidViewGroupTemplate" inherits="VerticalLayoutFrame" virtual="true">
        <KeyValues>
            <KeyValue key="fixedWidth" value="138" type="number" />
            <KeyValue key="topPadding" value="20" type="number" />
        </KeyValues>

        <Layers>  
            <Layer level="OVERLAY">
                <FontString parentKey="Title" font="GameFontHighlight" text="Group">
                    <Size y="20" />
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                        <Anchor point="TOPRIGHT"/>
                    </Anchors>
                </FontString>
            </Layer>

            <Layer level="BACKGROUND">
                <Texture parentKey="HeaderColor">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Title"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Title"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="Space1Color">
                    <Size y="30" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Title" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.Title" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="Space2Color">
                    <Size y="30" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Space1Color" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.Space1Color" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="Space3Color">
                    <Size y="30" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Space2Color" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.Space2Color" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="Space4Color">
                    <Size y="30" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Space3Color" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.Space3Color" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="Space5Color">
                    <Size y="30" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Space4Color" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.Space4Color" relativePoint="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>
</Ui>