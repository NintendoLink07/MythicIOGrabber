<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_ProgressDungeonCharacterTemplate" inherits="MIOG_ProgressCharacterTemplate, HorizontalLayoutFrame" virtual="true">
        <KeyValues>
            <KeyValue key="spacing" value="2" />
            <KeyValue key="leftPadding" value="125" type="number" />
        </KeyValues>

        <Frames>
            <Frame parentKey="Dungeon1" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon2" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon3" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon4" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon5" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon6" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon7" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>

            <Frame parentKey="Dungeon8" inherits="MIOG_ProgressDungeonTemplate">
            </Frame>
        </Frames>

        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Score" font="GameFontHighlight" justifyH="LEFT" justifyV="MIDDLE" text="CURRENT SCORE">
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" x="2" y="1"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="ScoreIncrease" font="GameFontHighlight" justifyH="LEFT" justifyV="MIDDLE" text="SCORE INC" hidden="true">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Score" relativePoint="RIGHT" x="10" />
                    </Anchors>
                </FontString>
            </Layer>
            
        </Layers>
    </Frame>
</Ui>