<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_VaultProgressRaidTemplate" inherits="MIOG_VaultProgressTemplate" virtual="true">
        <KeyValues>
			<KeyValue key="stride" value="6" type="number"/>
