        <Layers>
           <Layer level="ARTWORK">
                <Texture parentKey="Progress6" atlas="activities-reward-border">
                    <Size x="20" y="20" />
                    <KeyValues>
                        <KeyValue key="layoutIndex" value="1" type="number" />
                    </KeyValues>
                </Texture>

                <Texture parentKey="Progress6Check" atlas="activities-icon-checkmark" hidden="true">
                    <Size x="10" y="10" />
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Progress6" />
                    </Anchors>
                </Texture>

                <Texture parentKey="Progress5" atlas="activities-complete-diamond">
                    <Size x="20" y="20" />
                    <KeyValues>
                        <KeyValue key="layoutIndex" value="2" type="number" />
                    </KeyValues>
                </Texture>

                <Texture parentKey="Progress4" atlas="activities-reward-border">
                    <Size x="20" y="20" />
                    <KeyValues>
                        <KeyValue key="layoutIndex" value="3" type="number" />
                    </KeyValues>
                </Texture>

                <Texture parentKey="Progress4Check" atlas="activities-icon-checkmark" hidden="true">
                    <Size x="10" y="10" />
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Progress4" />
