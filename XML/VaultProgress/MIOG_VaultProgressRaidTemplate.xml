                <Texture parentKey="Progress2Check" atlas="activities-icon-checkmark" hidden="true">
                    <Size x="10" y="10" />
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Progress2" />
                    </Anchors>
                </Texture>

                <Texture parentKey="Progress1" atlas="activities-complete-diamond">
                    <Size x="20" y="20" />
                    <KeyValues>
                        <KeyValue key="layoutIndex" value="6" type="number" />
                    </KeyValues>
                </Texture>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Progress6Text" inherits="GameFontHighlight" outline="THICK">
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Progress6" relativePoint="TOP" y="2"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="Progress4Text" inherits="GameFontHighlight" outline="THICK">
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent.Progress4" relativePoint="TOP" y="2"/>
