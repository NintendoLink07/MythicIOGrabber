<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Button name="MIOG_Test" parentKey="MIOG_TESTBUTTON" inherits="SecureActionButtonTemplate" parent="UIParent" registerForClicks="LeftButtonDown">
        <Attributes>
            <Attribute name="type" type="string" value="macro"/>
            <Attribute name="macrotext" type="string" value="/run PVEFrame_OnEvent(PVEFrame, AJ_PVP_ACTION, 1018)"/>
        </Attributes>

        <Size x="64" y="64"/>

        <Layers>
            <Layer level="OVERLAY">
                <Texture name="$parentIcon" file="Interface\Icons\Spell_Nature_Regeneration" setAllPoints="true" />
            </Layer>
        </Layers>
        <Anchors>
            <Anchor point="TOPLEFT" />
        </Anchors>
    </Button>
</Ui>