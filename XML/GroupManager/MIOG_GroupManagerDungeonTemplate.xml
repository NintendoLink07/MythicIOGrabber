<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GroupManagerDungeonTemplate" virtual="true">
        <Size x="67" y="27" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon" setAllPoints="true" propagateMouseInput="Motion"/>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Level" font="SystemFont_Shadow_Med2" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Scripts>
            <OnEnter>
                if(self.dungeonName) then
                    GameTooltip:SetOwner(self, "ANCHOR_RIGHT")
                    GameTooltip:SetText(self.dungeonName)
                    GameTooltip:Show()
                end
            </OnEnter>
            <OnLeave function="GameTooltip_Hide" />
        </Scripts>
    </Frame>
</Ui>