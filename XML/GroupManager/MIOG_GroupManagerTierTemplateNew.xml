                            <Size x="80" />
                            <Anchors>
                                <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" x="3" />
                            </Anchors>
                        </FontString>

                        <FontString parentKey="Progress1" font="SystemFont_Shadow_Med1" justifyH="LEFT">
