..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GroupManagerTierTemplateNew" propagateMouseInput="Both" virtual="true">
        <Size y="49" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Background" horizTile="true" vertTile="true" setAllPoints="true" />
            </Layer>
