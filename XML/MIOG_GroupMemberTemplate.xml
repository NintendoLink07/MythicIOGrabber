<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GroupMemberTemplate" virtual="true" flattenRenderLayers="true">
        <Size x="17" y="17" />

        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon" enableMouseClicks="true">
                    <Size x="13" y="13" />

                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="2" y="-2" />
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="BORDER">
                <Texture parentKey="Border">
                    <Size x="17" y="17" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Icon" relativePoint="TOPLEFT" x="-2" y="2" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>

        <Scripts>
            <OnLeave>
                GameTooltip:Hide()
            </OnLeave>
        </Scripts>

    </Frame>
</Ui>