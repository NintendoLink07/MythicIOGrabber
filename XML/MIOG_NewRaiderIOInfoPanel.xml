            </Frame>

            <Frame parentKey="Raids">
                <Size y="120" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.MythicPlus" relativePoint="TOPRIGHT" x="5" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOPRIGHT" />
