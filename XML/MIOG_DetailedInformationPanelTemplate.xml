                        <Scripts>
                            <OnClick>
                                self:GetParent():GetParent().PanelContainer.MythicPlus:Hide()
                                self:GetParent():GetParent().PanelContainer.Raid:Show()
                                self:GetParent():GetParent():MarkDirty()

                            </OnClick>
                        </Scripts>
                    </Button>
                </Frames>
            </Frame>
