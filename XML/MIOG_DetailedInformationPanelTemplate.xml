..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_DetailedInformationPanelTemplate" inherits="ResizeLayoutFrame, BackdropTemplate" virtual="true">
        <KeyValues>
            <KeyValue key="minimumHeight" value="200" type="number" />
        </KeyValues>

        <Frames>
            <Frame parentKey="TabPanel" inherits="BackdropTemplate">
                <Size y="20" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOPRIGHT" />
                </Anchors>

                <Frames>
                    <Button parentKey="MythicPlusTabButton" inherits="UIPanelButtonTemplate" text="Mythic+">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOP" />
                        </Anchors>
                        <Scripts>
                            <OnClick>
                                self:GetParent():GetParent().PanelContainer.MythicPlus:Show()
                                self:GetParent():GetParent().PanelContainer.Raid:Hide()
                                self:GetParent():GetParent():MarkDirty()

                            </OnClick>
                        </Scripts>
                    </Button>
                    <Button parentKey="RaidTabButton" inherits="UIPanelButtonTemplate" text="Raid">
                        <Size y="20" />
