<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_StatisticsPageListEntryTemplate" virtual="true">
        <Size y="20" />
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Name" font="GameFontHighlight" justifyH="LEFT" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="LEFT"/>
                        <Anchor point="RIGHT" x="-40"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="Value" font="GameFontHighlight" justifyH="LEFT" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Name" relativePoint="RIGHT" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>