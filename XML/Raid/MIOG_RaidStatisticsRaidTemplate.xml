<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_RaidStatisticsRaidTemplate" inherits="BackdropTemplate" virtual="true">
        <Size y="32" />

        <Frames>
            <Frame parentKey="Normal">
                <Size x="45" y="20" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT"/>
                </Anchors>

                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="Current" font="Interface\Addons\MythicIOGrabber\res\fonts\LiberationMono-Regular.ttf" justifyH="CENTER" justifyV="MIDDLE" text="0/8" outline="NORMAL" >
                            <FontHeight val="14" />
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="5"/>
                            </Anchors>
                        </FontString>

                        <FontString parentKey="Previous" font="LibMono" justifyH="CENTER" justifyV="MIDDLE" text="NORMAL" hidden="true">
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="-10"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>

            <Frame parentKey="Heroic">
                <Size x="45" y="20" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent.Normal" relativePoint="RIGHT"/>
                </Anchors>

                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="Current" font="Interface\Addons\MythicIOGrabber\res\fonts\LiberationMono-Regular.ttf" justifyH="CENTER" justifyV="MIDDLE" text="0/8" outline="NORMAL" >
                            <FontHeight val="14" />
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="5"/>
                            </Anchors>
                        </FontString>

                        <FontString parentKey="Previous" font="LibMono" justifyH="CENTER" justifyV="MIDDLE" text="NORMAL" hidden="true">
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="-10"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>

            <Frame parentKey="Mythic">
                <Size x="45" y="20" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent.Heroic" relativePoint="RIGHT"/>
                </Anchors>

                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="Current" font="Interface\Addons\MythicIOGrabber\res\fonts\LiberationMono-Regular.ttf" justifyH="CENTER" justifyV="MIDDLE" text="0/8" outline="NORMAL" >
                            <FontHeight val="14" />
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="5"/>
                            </Anchors>
                        </FontString>

                        <FontString parentKey="Previous" font="LibMono" justifyH="CENTER" justifyV="MIDDLE" text="NORMAL" hidden="true">
                            <Anchors>
                                <Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER" y="-10"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
    </Frame>
</Ui>