<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_NewRaiderIOInfoPanelDungeonTemplate" virtual="true">
        <Layers>
            <Layer level="BORDER">
                <Texture parentKey="Border">
                    <Size x="18" y="18" />
                    <Color r="0" g="0" b="0" a="1" />
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" x="1"/>
                    </Anchors>
                </Texture>
            </Layer>
            
            <Layer level="ARTWORK">
                <Texture parentKey="Icon" enableMouseClicks="true">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Border" relativePoint="TOPLEFT" x="1" y="-1" />
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Border" relativePoint="BOTTOMRIGHT" x="-1" y="1" />
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Name" font="SystemFont_Shadow_Med1" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Icon" relativePoint="RIGHT" x="5" />
                    </Anchors>
                </FontString>

                <FontString parentKey="Level" font="SystemFont_Shadow_Med1" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Name" relativePoint="LEFT" x="45" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>