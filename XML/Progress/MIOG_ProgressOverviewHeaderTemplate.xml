<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_ProgressOverviewHeaderTemplate" mixin="ProgressHeaderMixin" virtual="true">
        <Size y="40" />

        <Layers>
			<Layer level="BORDER">
				<Texture parentKey="GuildBannerBackground" atlas="guildfinder-card-guildbanner-background" hidden="true">
					<Size y="192"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="-8" y="5"/>
						<Anchor point="TOPRIGHT" x="8" y="5"/>
					</Anchors>
				</Texture>
			</Layer>
            
			<Layer level="BORDER" textureSubLevel="-2">
				<Texture parentKey="GuildBannerShadow" atlas="guildfinder-card-guildbanner-shadow" hidden="true">
					<Size y="192"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativeKey="$parent.GuildBannerBackground"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.GuildBannerBackground"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER" textureSubLevel="2">
				<Texture parentKey="GuildBannerBorder" atlas="guildfinder-card-guildbanner-border" hidden="true">
					<Size y="192"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativeKey="$parent.GuildBannerBackground"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.GuildBannerBackground"/>
					</Anchors>
				</Texture>
			</Layer>
        </Layers>
    </Frame>
</Ui>