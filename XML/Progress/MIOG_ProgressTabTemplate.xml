<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_ProgressTabTemplate" mixin="ProgressMixin" virtual="true">
        <Frames>
            <Frame parentKey="Menu" inherits="VerticalLayoutFrame">
                <KeyValues>
                    <Keyvalue key="fixedWidth" value="84" type="number" />
                    <Keyvalue key="fixedHeight" value="103" type="number" />
                </KeyValues>

                <Anchors>
                    <Anchor point="TOPLEFT"/>
                </Anchors>

                <Frames>
                    <Button parentKey="OverviewButton" name="Overview" inherits="MIOG_ProgressSelectionButtonTemplate" text="Overview">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="1" type="number" />
                        </KeyValues>
                    </Button>

                    <Button parentKey="MythicPlusButton" name="MythicPlus" inherits="MIOG_ProgressSelectionButtonTemplate" text="Mythic+">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="2" type="number" />
                        </KeyValues>
                    </Button> 

                    <Button parentKey="RaidButton" name="Raid" inherits="MIOG_ProgressSelectionButtonTemplate" text="Raid">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="3" type="number" />
                        </KeyValues>
                    </Button>

                    <Button parentKey="PVPButton" name="PVP" inherits="MIOG_ProgressSelectionButtonTemplate" text="PVP">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="4" type="number" />
                        </KeyValues>
                    </Button> 
                    
                    <DropdownButton parentKey="VisibilityDropdown" inherits="WowStyle1DropdownTemplate">
                        <Size y="25" />

                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="5" type="number" />
                            <Keyvalue key="expand" value="true" type="boolean" />
                        </KeyValues>
                    </DropdownButton>
                </Frames>
            </Frame>
            
            <Frame parentKey="Info" inherits="VerticalLayoutFrame">
                <KeyValues>
                    <Keyvalue key="fixedWidth" value="84" type="number" />
                    <Keyvalue key="rightPadding" value="1" type="number" />
                </KeyValues>
                
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.Menu" relativePoint="BOTTOMLEFT"/>
                    <Anchor point="TOPRIGHT" relativeKey="$parent.Menu" relativePoint="BOTTOMRIGHT"/>
                </Anchors>

                <Frames>
                    <Frame parentKey="HonorLevel" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="1" type="number" />
                            <Keyvalue key="visual" value="light" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="HonorProgress" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="2" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="ItemLevel" name="Itemlevel" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="3" type="number" />
                            <Keyvalue key="visual" value="light" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="MythicPlusRating" name="Mythic+" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="10" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="ResilientLevel" name="Resilient" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="11" type="number" />
                            <Keyvalue key="visual" value="light" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="Normal" name="Normal" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="20" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="Heroic" name="Heroic" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="21" type="number" />
                            <Keyvalue key="visual" value="light" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="Mythic" name="Mythic" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="22" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>
                    
                    <Frame parentKey="GreatVault" name="Great Vault" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="30" type="number" />
                            <Keyvalue key="visual" value="dark" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>
                    
                    <Frame parentKey="GreatVaultRaids" name="Raids" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="31" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="GreatVaultDungeons" name="Dungeons" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="32" type="number" />
                            <Keyvalue key="visual" value="light" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>

                    <Frame parentKey="GreatVaultWorld" name="World" inherits="MIOG_ProgressOverviewColorCellTemplate">
                        <KeyValues>
                            <Keyvalue key="layoutIndex" value="33" type="number" />
                            <Keyvalue key="visual" value="semi" type="string" />
                            <Keyvalue key="align" value="right" type="string" />
                        </KeyValues>
                    </Frame>
                </Frames>
            </Frame>

            <Frame parentKey="Overview" mixin="ProgressOverviewMixin">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.Menu" relativePoint="TOPRIGHT"/>
                    <Anchor point="BOTTOMRIGHT"/>
                </Anchors>

                <Frames>
                    <EventFrame parentKey="ScrollBar" inherits="MIOG_MinimalHorizontalScrollBarTemplate">
                        <Size y="16" />

                        <KeyValues> 
                            <KeyValue key="minThumbExtent" value="10" type="number"/>
                        </KeyValues>

                        <Anchors>
                            <Anchor point="BOTTOMLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT"/>
                            <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT"/>
                        </Anchors>

                    </EventFrame>

                    <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                        <Anchors>
                            <!--<Anchor point="TOPLEFT" relativeKey="$parent.HeaderContainer" relativePoint="BOTTOMLEFT"/>-->
                            <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT"/>
                            <Anchor point="BOTTOMRIGHT" relativeKey="$parent.ScrollBar" relativePoint="TOPRIGHT"/>
                        </Anchors>
                    </Frame>

                </Frames>

                <Scripts>
                    <OnLoad method="OnLoad" />
                    <OnShow method="OnShow" />
                </Scripts>
            </Frame>

            <Frame parentKey="MythicPlus" mixin="ProgressDungeonMixin" hidden="true">
                <Anchors>
                    <Anchor point="TOPLEFT" />
                    <Anchor point="BOTTOMRIGHT"/>
                </Anchors>
                
                <Frames>
                    <Frame parentKey="HeaderContainer">
                        <Size y="135"/>

                        <Anchors>
                            <Anchor point="TOPLEFT"/>
                            <Anchor point="TOPRIGHT"/>
                        </Anchors>
                    </Frame>

                    <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.HeaderContainer" relativePoint="BOTTOMLEFT"/>
                            <Anchor point="BOTTOMRIGHT"/>
                        </Anchors>
                    </Frame>
                </Frames>
                
                <Scripts>
                    <OnShow method="OnShow" />
                    <OnLoad method="OnLoad" />
                </Scripts>
            </Frame>
            
            <Frame parentKey="Raid" mixin="ProgressRaidMixin" hidden="true">
                <Anchors>
                    <Anchor point="TOPLEFT" />
                    <Anchor point="BOTTOMRIGHT"/>
                </Anchors>
                
                <Frames>
                    <Frame parentKey="HeaderContainer">
                        <Size y="135"/>

                        <Anchors>
                            <Anchor point="TOPLEFT"/>
                            <Anchor point="TOPRIGHT"/>
                        </Anchors>
                    </Frame>

                    <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.HeaderContainer" relativePoint="BOTTOMLEFT"/>
                            <Anchor point="BOTTOMRIGHT"/>
                        </Anchors>
                    </Frame>
                </Frames>
                
                <Scripts>
                    <OnShow method="OnShow" />
                    <OnLoad method="OnLoad" />
                </Scripts>
            </Frame>
            
            <Frame parentKey="PVP" mixin="ProgressPVPMixin" hidden="true">
                <Anchors>
                    <Anchor point="TOPLEFT" />
                    <Anchor point="BOTTOMRIGHT"/>
                </Anchors>
                
                <Frames>
                    <Frame parentKey="HeaderContainer">
                        <Size y="135"/>

                        <Anchors>
                            <Anchor point="TOPLEFT"/>
                            <Anchor point="TOPRIGHT"/>
                        </Anchors>
                    </Frame>

                    <Frame parentKey="ScrollBox" inherits="WowScrollBoxList">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.HeaderContainer" relativePoint="BOTTOMLEFT"/>
                            <Anchor point="BOTTOMRIGHT"/>
                        </Anchors>
                    </Frame>
                </Frames>
                
                <Scripts>
                    <OnLoad method="OnLoad" />
                    <OnShow method="OnShow" />
                </Scripts>
            </Frame>
        </Frames>

        <Scripts>
            <OnLoad method="OnLoad" />
            <OnShow method="OnShow" />
            <OnEvent method="OnEvent" />
        </Scripts>
    </Frame>
</Ui>