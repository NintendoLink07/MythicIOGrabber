<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_ProgressRaidCellTemplate" mixin="ProgressMythicPlusCellMixin" virtual="true" hidden="true">
        <Size x="54" y="20"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Heroic" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="Normal" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="LEFT"/>
                        <Anchor point="RIGHT" relativeKey="$parent.Heroic" relativePoint="LEFT"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="Mythic" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="RIGHT"/>
                        <Anchor point="LEFT" relativeKey="$parent.Heroic" relativePoint="RIGHT"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Scripts>
            <OnLeave function="GameTooltip_Hide" />
        </Scripts>
    </Frame>
</Ui>