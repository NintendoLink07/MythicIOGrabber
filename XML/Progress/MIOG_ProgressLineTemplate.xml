<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_ProgressLineTemplate" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="DividerShadow" atlas="spec-columndivider" alpha="0.7">
                    <Anchors>
                        <Anchor point="TOPLEFT" />
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="ARTWORK">
                <Texture parentKey="DividerTop" atlas="dragonflight-weeklyrewards-divider" alpha="0.5">
