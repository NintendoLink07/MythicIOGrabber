                        <Anchor point="TOPLEFT" y="-3"/>
                        <Anchor point="TOPRIGHT" y="-3"/>
                    </Anchors>
                </Texture>

                <Texture parentKey="DividerBottom" atlas="dragonflight-weeklyrewards-divider" alpha="0.5">
                    <Size y="6" />
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" y="3"/>
                        <Anchor point="BOTTOMRIGHT" y="3"/>
