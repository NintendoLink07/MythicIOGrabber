<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_StatisticsPage" virtual="true">
        <Frames>
            <Frame parentKey="ScrollBox" inherits="WowScrollBoxList" >
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT"/>
                    <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-20"/>
                </Anchors>
            </Frame>

            <EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.ScrollBox" relativePoint="TOPRIGHT"/>
                    <Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollBox" relativePoint="BOTTOMRIGHT" />
                </Anchors>
            </EventFrame>
        </Frames>
    </Frame>
</Ui>