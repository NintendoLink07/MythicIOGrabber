<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_DungeonRowTemplate" inherits="BackdropTemplate" virtual="true">
        <Size y="20" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon">
                    <Size x="16" y="16" />

                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Name" font="SystemFont_Shadow_Med1" justifyH="LEFT">
                    <Size x="50" />
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Icon" relativePoint="RIGHT" x="2" y="-1"/>
                    </Anchors>
                </FontString>

                <FontString parentKey="Primary" font="SystemFont_Shadow_Med1" justifyH="LEFT">
                    <Size x="45" />
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" x="65"/>
                    </Anchors>
                </FontString>
                
                <FontString parentKey="Secondary" font="SystemFont_Shadow_Med1" justifyH="LEFT">
                    <Size x="45" />
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" x="105"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>