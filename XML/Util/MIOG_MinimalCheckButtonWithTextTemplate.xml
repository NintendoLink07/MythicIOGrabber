<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_MinimalCheckButtonWithTextTemplate" virtual="true">
        <Frames>
            <CheckButton parentKey="Button" inherits="MIOG_MinimalCheckButtonTemplate">
                <Size x="22" y="22" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT"/>
                </Anchors>
            </CheckButton>
        </Frames>

        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Button" relativePoint="RIGHT" x="2"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>