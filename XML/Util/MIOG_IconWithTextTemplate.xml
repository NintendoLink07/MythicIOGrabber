<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_IconWithTextTemplate" virtual="true">
        <Size x="40" y="20" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon">
                    <Size x="20" y="20" />
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="OVERLAY" textureSubLevel="-1">
                <FontString parentKey="Text" font="SystemFont_Shadow_Med2" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Icon" relativePoint="RIGHT" x="3"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>