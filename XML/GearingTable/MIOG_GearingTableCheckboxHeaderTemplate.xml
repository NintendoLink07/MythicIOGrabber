<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GearingTableCheckboxHeaderTemplate" mixin="GearingTableHeaderMixin" virtual="true">
        <Frames>
            <CheckButton parentKey="Checkbox" inherits="MIOG_MinimalCheckButtonTemplate" checked="true">
                <Size x="14" y="14" />
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT"/>
                </Anchors>
            </CheckButton>
        </Frames>

        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.Checkbox" relativePoint="RIGHT"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>