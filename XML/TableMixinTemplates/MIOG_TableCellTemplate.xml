<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_TableCellTemplate" inherits="ResizeLayoutFrame" virtual="true">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Background" setAllPoints="true" />
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Text" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE" setAllPoints="true" />
            </Layer>
        </Layers>
    </Frame>
</Ui>