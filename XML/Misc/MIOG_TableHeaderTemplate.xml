<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_TableHeaderTemplate" mixin="TableHeaderButtonMixin" virtual="true">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="BackgroundColor" setAllPoints="true"/>
            </Layer>

            <Layer level="OVERLAY">
                <FontString parentKey="Text" font="GameFontHighlight" justifyH="CENTER" justifyV="MIDDLE" outline="NORMAL">
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Scripts>
            <OnShow method="OnShow" />
        </Scripts>
    </Frame>
</Ui>