<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_DetailedInformationPanelSinglePanelTemplate" inherits="BackdropTemplate" virtual="true">
        <Frames>
            <Frame parentKey="Left">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                    <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOM" />
                </Anchors>
            </Frame>

            <Frame parentKey="Right">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOP" />
                    <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" />
                </Anchors>
                <Frames>
                    <Frame parentKey="1">
                        <Size y="100" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOPRIGHT" />
                        </Anchors>

                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="FontString" font="LibMono" justifyH="LEFT" justifyV="TOP" wordwrap="true" nonspacewrap="true">
                                    <FontHeight val="11"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" y="-3"/>
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>

                    <Frame parentKey="2">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.1" relativePoint="BOTTOMLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent.1" relativePoint="BOTTOMRIGHT" />
                        </Anchors>

                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="FontString" font="LibMono" justifyH="LEFT">
                                    <FontHeight val="11"/>
                                    <Anchors>
                                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>
                    
                    <Frame parentKey="3">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.2" relativePoint="BOTTOMLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent.2" relativePoint="BOTTOMRIGHT" />
                        </Anchors>

                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="FontString" font="LibMono" justifyH="LEFT">
                                    <FontHeight val="11"/>
                                    <Anchors>
                                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>
                    
                    <Frame parentKey="4">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.3" relativePoint="BOTTOMLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent.3" relativePoint="BOTTOMRIGHT" />
                        </Anchors>

                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="FontString" font="LibMono" justifyH="LEFT">
                                    <FontHeight val="11"/>
                                    <Anchors>
                                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>
                    
                    <Frame parentKey="5">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.4" relativePoint="BOTTOMLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent.4" relativePoint="BOTTOMRIGHT" />
                        </Anchors>

                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="FontString" font="LibMono" justifyH="LEFT">
                                    <FontHeight val="11"/>
                                    <Anchors>
                                        <Anchor point="LEFT" relativeKey="$parent" relativePoint="LEFT" />
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </Frame>
                </Frames>
            </Frame>
        </Frames>
    </Frame>
</Ui>