<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_QueueManager" mixin="QueueManagerMixin" inherits="InsetFrameTemplate3" frameLevel="5" virtual="true">
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Title" font="GameFontHighlight" text="Your active queues will be displayed here">
                    <Anchors>
                        <Anchor point="LEFT" />
                        <Anchor point="RIGHT" />
                    </Anchors>
                </FontString>

                <FontString parentKey="FakeApps" font="GameFontHighlight">
                    <Anchors>
                        <Anchor point="CENTER" relativeKey="$parent" relativePoint="TOPLEFT" x="3" y="-4"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Frames>
            <Frame parentKey="ScrollBox" inherits="WowScrollBoxList" setAllPoints="true" />
            <EventFrame parentKey="ScrollBar" inherits="MinimalScrollBar" />
        </Frames>

        <Scripts>
            <OnLoad method="OnLoad" />
            <OnEvent method="OnEvent" />
        </Scripts>
    </Frame>
</Ui>