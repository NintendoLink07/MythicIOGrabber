<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_StatisticsPageListHeaderTemplate" virtual="true">
        <Size y="20" />
        
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Name" font="GameFontHighlight" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL" text="AbilityName" maxLines="1">
                    <Anchors>
                        <Anchor point="BOTTOM" relativeKey="$parent" relativePoint="BOTTOM" y="2"/>
                    </Anchors>
                </FontString>
            </Layer>

            <Layer level="ARTWORK">
                <Texture parentKey="Icon" hidden="true">
                    <Size x="12" y="12" />
                    <Anchors>
                        <Anchor point="RIGHT" relativeKey="$parent.Name" relativePoint="LEFT" x="-2"/>
                    </Anchors>
                </Texture>
            </Layer>
            
            <Layer level="BORDER">
                <Texture parentKey="Divider1" atlas="dragonflight-weeklyrewards-divider">
                    <Size y="5" />
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" x="1"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-1"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>
</Ui>