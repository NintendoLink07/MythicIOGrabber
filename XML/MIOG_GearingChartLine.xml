<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOG_GearingChartLine" inherits="GridLayoutFrame" virtual="true">
        <KeyValues>
            <KeyValue key="isHorizontal" value="true" type="boolean" />
            <KeyValue key="layoutFramesGoingRight" value="true" type="boolean" />
            <KeyValue key="stride" value="7" type="number" />
            <KeyValue key="childXPadding" value="10" type="number" />
            <KeyValue key="fixedHeight" value="12" type="number" />
        </KeyValues>

        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="ItemLevel" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="35" />
                </FontString>

                <FontString parentKey="Track" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="105" />
                </FontString>

                <FontString parentKey="Raid" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="55" />
                </FontString>

                <FontString parentKey="Dungeon" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="70" />
                </FontString>

                <FontString parentKey="DungeonVault" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="70" />
                </FontString>

                <FontString parentKey="Other" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="60" />
                </FontString>

                <FontString parentKey="Progress" font="SystemFont_Shadow_Med1" justifyH="LEFT" justifyV="MIDDLE" outline="NORMAL">
                    <Size x="150" />
                </FontString>
            </Layer>

            <Layer level="ARTWORK">
                <Texture parentKey="Divider" atlas="dragonflight-weeklyrewards-divider">
                    <Size y="5" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" />
                        <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>
</Ui>