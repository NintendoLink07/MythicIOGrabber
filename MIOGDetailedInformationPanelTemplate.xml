<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Frame name="MIOGDetailedInformationPanelTemplate" inherits="ResizeLayoutFrame, BackdropTemplate" virtual="true">
        <Frames>
            <Frame parentKey="TabPanel" inherits="BackdropTemplate">
                <Size y="20" />
                <!--<Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOPRIGHT" />
                </Anchors>-->

                <Frames>
                    <Button parentKey="MythicPlusTabButton" inherits="UIPanelButtonTemplate" text="Mythic+">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOP" />
                        </Anchors>
                    </Button>
                    <Button parentKey="RaidTabButton" inherits="UIPanelButtonTemplate" text="Raid">
                        <Size y="20" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOP" />
                            <Anchor point="TOPRIGHT" relativeKey="$parent" relativePoint="TOPRIGHT" />
                        </Anchors>
                    </Button>
                </Frames>
            </Frame>
            <Frame parentKey="MythicPlusPanel" inherits="BackdropTemplate" hidden="true">
                <Size y="180" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.TabPanel" relativePoint="BOTTOMLEFT" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent.TabPanel" relativePoint="BOTTOM" />
                </Anchors>
            </Frame>
            <Frame parentKey="RaidPanel" inherits="BackdropTemplate" hidden="true">
                <Size y="180" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.TabPanel" relativePoint="BOTTOMLEFT" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent.TabPanel" relativePoint="BOTTOM" />
                </Anchors>
            </Frame>
            <Frame parentKey="GeneralInfoPanel" inherits="BackdropTemplate" hidden="true">
                <Size y="180" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.TabPanel" relativePoint="BOTTOM" />
                    <Anchor point="TOPRIGHT" relativeKey="$parent.TabPanel" relativePoint="BOTTOMRIGHT" />
                </Anchors>
            </Frame>
        </Frames>
    </Frame>
</Ui>